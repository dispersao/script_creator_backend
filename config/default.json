{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "apiPath": "api",
  "paginate": {
    "default": 100,
    "max": 100
  },
  "mysql": "mysql://root:password@127.0.0.1/dispersao_sequences2",
  "authentication": {
    "secret": "JWT_SECRET",
    "strategies": [
      "jwt",
      "local"
